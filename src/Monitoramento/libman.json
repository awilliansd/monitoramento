{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    },
    {
      "library": "knockout@3.4.2",
      "destination": "wwwroot/lib/knockout/"
    },
    {
      "library": "modernizr@2.8.3",
      "destination": "wwwroot/lib/modernizr/"
    },
    {
      "library": "popper.js@1.14.6",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "library": "Chart.js@2.7.3",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.1.0",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}